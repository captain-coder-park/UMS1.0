{% extends 'base.html.twig' %}

{% block body %}
<h4 class="common_head4">Update Reference Details</h4>
<div class="container">
    <div class="row">
        {{ form_start(form) }}
            {{ form_row(form.fullName) }}
            {{ form_row(form.phone) }}
            {{ form_row(form.email) }}
            {{ form_row(form.city) }}
            <br>
            <div class="form-group pull-right">
                <button type="submit" class="btn btn-primary form">Submit</button>
            </div>
        {{ form_end(form) }}
    </div>
</div>
{% endblock %}
